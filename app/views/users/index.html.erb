<div class="container">
  <div class="row">
    <h1>所有使用者</h1>
    <div class="col-md-12">
      <div class="col-md-4">
        <% @users.each do |user| %>
          <h4><%= user.name %></h4>
          <%= image_tag user.avatar_url ,width: "200px" %>
        <% end %>
      </div>
    </div>
    
  </div>
</div>
